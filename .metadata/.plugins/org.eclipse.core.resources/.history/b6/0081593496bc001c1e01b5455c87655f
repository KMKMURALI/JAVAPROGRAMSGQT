class Calculator{
	/**
	 * @parm a
	 * @parm b
	 * description: the add() having both 2inputs and outputs and returning a integer value
	 * return type: int
	 */
	  void add() {
		int a = 100;
		 int b = 200;
		int c = a+b;
		System.out.println(c);
		
	}

}
